    "rollup-plugin-typescript2": "^0.30.0",
    "semantic-release": "^17.4.7"
  },
  "dependencies": {
    "camelcase": "^6.2.0"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.11.1",
    "**/@typescript-eslint/parser": "^4.11.1",
    "**/jest": "^26.6.3",
    "**/ts-jest": "^26.4.4",
    "**/typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pffigueiredo/react-formgen.git"
